@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prosopa: <http://purl.archive.org/purl/prosopa/ontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix shaclmate: <http://minorg.github.io/shaclmate/ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://schema.org/>
    shaclmate:tsFeatureExclude shaclmate:_TsFeature_Sparql ;
    a owl:Ontology .

schema:CreativeWork
    shaclmate:abstract true ;
    shaclmate:name "CreativeWork" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Thing ;
    sh:property schema:CreativeWork-isBasedOn .

schema:CreativeWork-isBasedOn
    a sh:PropertyShape ;
    sh:name "isBasedOn" ;
    sh:nodeKind sh:IRI ;
    sh:path schema:isBasedOn .

schema:Enumeration
    shaclmate:abstract true ;
    shaclmate:name "Enumeration" ;
    a rdfs:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Intangible .

schema:Female
    a schema:GenderType ;
    rdfs:label "Female" .

schema:GenderType
    shaclmate:name "GenderType" ;
    a rdfs:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Enumeration ;
    sh:in (schema:Female
        schema:Male
    ) .

schema:ImageObject
    shaclmate:name "ImageObject" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:MediaObject .

schema:Intangible
    shaclmate:abstract true ;
    shaclmate:name "Intangible" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Thing .

schema:Male
    a schema:GenderType ;
    rdfs:label "Male" .

schema:MediaObject
    shaclmate:abstract true ;
    shaclmate:name "MediaObject" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:CreativeWork ;
    sh:property schema:MediaObject-contentUrl, schema:MediaObject-encodingFormat, schema:MediaObject-height, schema:MediaObject-width .

schema:MediaObject-contentUrl
    a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:name "contentUrl" ;
    sh:nodeKind sh:IRI ;
    sh:path schema:contentUrl .

schema:MediaObject-encodingFormat
    a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "encodingFormat" ;
    sh:path schema:encodingFormat .

schema:MediaObject-height
    a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:name "height" ;
    sh:node schema:QuantitativeValue ;
    sh:path schema:height .

schema:MediaObject-width
    a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:name "width" ;
    sh:node schema:QuantitativeValue ;
    sh:path schema:width .

schema:Occupation
    shaclmate:name "Occupation" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Intangible .

schema:Organization
    shaclmate:name "Organization" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Thing .

schema:Person
    shaclmate:name "Person" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Thing ;
    sh:property schema:Person-affiliations, schema:Person-birthDate, schema:Person-familyName, schema:Person-gender, schema:Person-givenName, schema:Person-hasOccupation, schema:Thing-images .

schema:Person-affiliations
    a sh:PropertyShape ;
    sh:name "affiliations" ;
    sh:node schema:Organization ;
    sh:path schema:affiliation .

schema:Person-birthDate
    a sh:PropertyShape ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:name "birthDate" ;
    sh:path schema:birthDate .

schema:Person-familyName
    a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "familyName" ;
    sh:path schema:familyName .

schema:Person-gender
    a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:name "gender" ;
    sh:or ([
            sh:node schema:GenderType
        ]
        [
            sh:datatype xsd:string
        ]
    ) ;
    sh:path schema:gender .

schema:Person-givenName
    a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "givenName" ;
    sh:path schema:givenName .

schema:Person-hasOccupation
    a sh:PropertyShape ;
    sh:name "hasOccupation" ;
    sh:node schema:Occupation ;
    sh:path schema:hasOccupation .

schema:QuantitativeValue
    shaclmate:name "QuantitiveValue" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:StructuredValue ;
    sh:property schema:QuantitativeValue-value .

schema:QuantitativeValue-maxValue
    a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxCount 1 ;
    sh:name "maxValue" ;
    sh:path schema:maxValue .

schema:QuantitativeValue-minValue
    a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxCount 1 ;
    sh:name "minValue" ;
    sh:path schema:minValue .

schema:QuantitativeValue-value
    a sh:PropertyShape ;
    sh:datatype xsd:decimal ;
    sh:maxCount 1 ;
    sh:name "value" ;
    sh:path schema:value .

schema:StructuredValue
    shaclmate:abstract true ;
    shaclmate:name "StructuredValue" ;
    a owl:Class, sh:NodeShape ;
    rdfs:subClassOf schema:Intangible .

schema:Thing
    shaclmate:abstract true ;
    shaclmate:name "Thing" ;
    a owl:Class, sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:property schema:Thing-description, schema:Thing-name, schema:Thing-sameAs, schema:Thing-url .

schema:Thing-description
    a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "description" ;
    sh:path schema:description .

schema:Thing-images
    a sh:PropertyShape ;
    sh:name "images" ;
    sh:node schema:ImageObject ;
    sh:path schema:image .

schema:Thing-name
    a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:name "name" ;
    sh:path schema:name .

schema:Thing-sameAs
    a sh:PropertyShape ;
    sh:name "sameAs" ;
    sh:nodeKind sh:IRI ;
    sh:path schema:sameAs .

schema:Thing-url
    a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:name "url" ;
    sh:nodeKind sh:IRI ;
    sh:path schema:url .

